{
  "exp_name": "example_random_augmentation",
  "agent": "MlpCIFAR",
  "dataset": "CIFAR10",
  "dl_model": "MLP",
  "ga_model": "GA_RandomModel",
  "mode": "train",

  "cuda": true,
  "gpu_device": 0,
  "seed": 1337,

  "batch_size": 128,

  "learning_rate": 0.001,
  "weight_decay": 0,
  "beta1": 0.5,
  "beta2": 0.999,

  "max_epoch": 20,

  "checkpoint_file": "checkpoint.pth.tar",

  "augmentations": ["rotate", "translate_horizontal"],
  "augmentation_ranges": [
    [-30, 30],
    [-0.1, 0.1]
  ],
  "population_size": 1,
  "crossover_rate": 0.9,
  "elitism_selection_rate": 0.5,
  "shuffle": false
}
